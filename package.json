{
  "name": "@bitrix24/b24rabbitmq",
  "version": "0.0.1",
  "author": "Bitrix",
  "description": "Library for integrating Bitrix24 applications with RabbitMQ",
  "keywords": [
    "Bitrix24",
    "RabbitMq",
    "typescript"
  ],
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitrix24/b24rabbitmq.git"
  },
  "homepage": "https://github.com/bitrix24/b24rabbitmq",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prepack": "pnpm run build",
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
  },
  "devDependencies": {
    "esbuild": "^0.25.4",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "eslint-config-unjs": "^0.4.2"
  },
  "peerDependencies": {
    "amqplib": "^0.10.8"
  },
  "resolutions": {},
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "ignoredBuiltDependencies": []
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": {
        "require": "./dist/commonjs/index.d.cts",
        "import": "./dist/esm/index.d.mts",
        "default": "./dist/esm/index.d.ts"
      },
      "default": {
        "require": "./dist/commonjs/index.cjs",
        "import": "./dist/esm/index.mjs",
        "default": "./dist/esm/index.mjs"
      }
    }
  },
  "main": "./dist/commonjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/esm/index.d.ts",
  "unpkg": "dist/umd/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
